<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="8dp">

    <ImageButton
        android:id="@+id/btn_lista"
        android:layout_width="124dp"
        android:layout_height="110dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_music_note_48" />

    <TextView
        android:id="@+id/nombreLista"
        android:layout_width="135dp"
        android:layout_height="31dp"
        android:layout_marginStart="64dp"
        android:layout_marginEnd="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_lista"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_lista"
        app:layout_constraintTop_toTopOf="@+id/btn_lista" />

</androidx.constraintlayout.widget.ConstraintLayout>
